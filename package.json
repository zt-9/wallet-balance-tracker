{
	"name": "ts-node-template",
	"version": "1.0.0",
	"description": "ts node template",
	"license": "MIT",
	"type": "module",
	"main": "dist/index.js",
	"files": [
		"LICENSE.md",
		"README.md",
		"dist/",
		"package.json"
	],
	"scripts": {
		"dev": "tsx src/index.ts",
		"dev:watch": "tsx watch src/index.ts",
		"build": "tsup",
		"start": "node dist/index.js",
		"fmt": "prettier --write .",
		"fmt:check": "prettier --check .",
		"fmt:src": "prettier --write .",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix",
		"preinstall": "npx only-allow pnpm",
		"typecheck": "tsc --noEmit",
		"prepare": "husky",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"dev-fetcher": "tsx bin/fetcher.ts",
		"dev-ui": "tsx src/ui/terminal/standalone.tsx"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write",
		"src/**/*.{ts,js}": [
			"eslint"
		]
	},
	"dependencies": {
		"better-sqlite3": "^11.9.1",
		"consola": "^3.4.2",
		"dotenv": "^16.5.0",
		"ink": "^5.2.1",
		"ink-spinner": "^5.0.0",
		"ink-table": "^3.1.0",
		"limiter": "^3.0.0",
		"react": "^18.3.1",
		"toml": "^3.0.0",
		"viem": "^2.30.0",
		"winston": "^3.17.0"
	},
	"devDependencies": {
		"@eslint/js": "9.24.0",
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "22.14.1",
		"@types/react": "^18.3.22",
		"eslint": "9.24.0",
		"husky": "9.1.7",
		"lint-staged": "15.5.1",
		"prettier": "3.5.3",
		"tsup": "8.4.0",
		"tsx": "^4.19.3",
		"typescript": "5.8.3",
		"typescript-eslint": "8.30.1",
		"vitest": "^3.1.1"
	},
	"engines": {
		"node": ">=22"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"better-sqlite3",
			"bufferutil",
			"utf-8-validate",
			"esbuild"
		]
	}
}
